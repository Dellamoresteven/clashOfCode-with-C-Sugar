@header
#inc e

using namespace std;

def vector<string> splitByDelim( s, d ) {
    var ret = vector std::string;
    size_t pos = 0;
    std::string token;
    while ((pos = s.find(d)) != std::string::npos) {
        token = s.substr(0, pos);
        ret.push(token);
        s.erase(0, pos + d.length());
    }
    printv(ret);
    return ret;
}

int main() {
    std::string S;
    getline(std::cin, S);
    map<int, int> sorted;
    var vecSplit = splitByDelim( S, " " );
    sorted[""]
}